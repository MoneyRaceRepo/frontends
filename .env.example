# Backend API URL
NEXT_PUBLIC_API_URL=http://localhost:3001

# Sui Network Configuration
NEXT_PUBLIC_SUI_NETWORK=testnet
NEXT_PUBLIC_SUI_RPC=https://fullnode.testnet.sui.io

# Google OAuth (for zkLogin)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id

# Contract Package ID (after deployment)
NEXT_PUBLIC_PACKAGE_ID=0x...

# Admin Cap ID (after deployment)
NEXT_PUBLIC_ADMIN_CAP_ID=0x...

# USDC Mock Coin Type
# Replace with your actual USDC mock package ID
# Format: 0x[package_id]::usdc::USDC
# If not set, defaults to SUI coin type (0x2::sui::SUI)
NEXT_PUBLIC_USDC_COIN_TYPE=0x2::sui::SUI
